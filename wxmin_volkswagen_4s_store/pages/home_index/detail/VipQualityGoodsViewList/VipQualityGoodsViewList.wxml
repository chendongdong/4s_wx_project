<view class="home_vip_goods_body">
    <view class="home_title_more_box">
        <view class="title_name">
            会员精品
        </view>
         <view  class="more_box" bindtap="goListFn">
            查看更多
            <image lazy-load="true"class="img_" src="{{ossImgAddre + 'next_gray.png'}}" mode="widthFix"/>
        </view>
    </view>
    <view class="detail_box">
        <view class="detail_box_one_item" wx:for="{{goodList}}" wx:key="{{item.id}}">
            <image lazy-load="true" bindtap="goDetailFn" data-id="{{item.id}}" class="detail_box_one_img" src="{{item.img}}" mode="aspectFill"/>
           <view class="detail_box_one_title">全包围汽车脚垫专用于途 观L探歌探岳宝来</view>
           <view class="detail_box_one_money">
              <text class="detail_box_one_money_unit">￥</text>
              <text class="detail_box_one_money_value">328.00</text>
          </view>
        </view>
    </view>
</view>