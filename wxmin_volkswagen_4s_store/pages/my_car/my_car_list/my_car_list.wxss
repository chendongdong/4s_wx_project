.my_car_list_body {
    width: 100%;
    height: 100%;
    background: white;
    overflow: hidden;
}
.my_car_list_body .detail_box{
    height: calc(100% - 184rpx);
    overflow: auto;
    width: 100%;
}
.my_car_list_body .detail_box .detail_ .title_box{
    background: #567BFF;
    display: flex;
    font-size: 28rpx;
    color: white;
    padding: 24rpx 20rpx;
    align-items: center;
    box-shadow: 0px 2px 4px 0px #567BFF;
}
.my_car_list_body .detail_box .detail_ .title_box .img_box .img_{
    width: 68rpx;
    height: 68rpx;
    border-radius: 100%;
    margin-right: 36rpx;
}
.my_car_list_body .detail_box .detail_{
    margin: 20rpx 22rpx;
    box-shadow: 0px 2px 5px 0px rgba(22,1,52,0.2);
}
.my_car_list_body .detail_box .detail_ .info_box{
    padding: 34rpx 46rpx 14rpx;
}
.my_car_list_body .detail_box .detail_ .info_box .info_ {
    display: flex;
}
.my_car_list_body .detail_box .detail_ .info_box .info_ .img_{
    width: 32rpx;
    margin-right: 36rpx;
}
.my_car_list_body .detail_box .detail_ .info_box .info_ .content_{
    color: #999999;
    font-size: 28rpx;
    margin-bottom: 22rpx;
}
.my_car_list_body .detail_box .detail_ .info_box .info_ .num_{
    color: #FF0000;
    font-size: 48rpx;
    font-weight: bold;
    margin: auto 15rpx;
    vertical-align: bottom;
}
.my_car_list_body .detail_box .no_data_box{
    text-align: center;
    overflow: hidden;
}
.my_car_list_body .detail_box .no_data_box .no_data_img{
    width: 398rpx;
    margin: 176rpx auto 35rpx;
}
.my_car_list_body .detail_box .no_data_box .no_data_text{
    color: #333333;
    font-size: 32rpx;
}
.my_car_list_body .bottom_box{
    position: absolute;
    bottom: 0;
    display: inline-block;
    width: 100%;
    background: white;
    margin-top: 10rpx;
}
.my_car_list_body .bottom_box .option_btn{
    margin-top: 30rpx;
}